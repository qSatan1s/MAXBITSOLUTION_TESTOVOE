{
  "name": "maxbitsolution_testovoe",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "repository": "https://github.com/qSatan1s/MAXBITSOLUTION_TESTOVOE",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest --config vitest.config.ts",
    "test:ui": "vitest --ui --config vitest.config.ts",
    "test:run": "vitest run --config vitest.config.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "vue-tsc --noEmit",
    "quality:check": "npm run type-check && npm run lint && npm run format:check && npm run test:run && npm run build"
  },
  "dependencies": {
    "@pinia/colada": "^0.17.1",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vueuse/core": "^13.5.0",
    "pinia": "^3.0.3",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.17.0",
    "@types/node": "^24.0.14",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.31.0",
    "jsdom": "^25.0.1",
    "prettier": "^3.6.2",
    "sass": "^1.89.2",
    "sass-loader": "^16.0.5",
    "typescript": "~5.8.3",
    "vite": "^7.0.4",
    "vitest": "^2.1.8",
    "vue-tsc": "^2.2.12"
